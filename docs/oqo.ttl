@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix oqo: <http://purl.org/oqo/> .
@prefix oqo-model: <http://purl.org/oqo/model#> .

<http://purl.org/oqo> a owl:Ontology ;
    rdfs:comment "Ontology Quality Ontology" ;
    owl:versionIRI <http://purl.org/oqo/version/1.0> .

oqo-model:Metric a owl:Class ;
    rdfs:label "Metric" ;
    rdfs:comment "A sample metric." .
